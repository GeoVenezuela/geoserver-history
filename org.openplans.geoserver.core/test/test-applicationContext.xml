<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="dummyService" 
		class="org.openplans.geoserver.DummyService"/>
		
	<bean id="dummyKvpBinding"
		class="org.openplans.geoserver.DummyKvpBinding">
		<constructor-arg>
			<list>
				<value type="java.lang.String">dummy</value>
			</list>
		</constructor-arg>
		<constructor-arg>
			<value type="java.lang.String">dummy</value>
		</constructor-arg>
	</bean>
	
	<bean id="test.dummyServiceMapping"
		class="org.openplans.geoserver.ServiceMapping">
		<constructor-arg>
			<map>
				<entry key="dummy" value="dummyService"/>
			</map>
		</constructor-arg>
	</bean>
	
</beans>
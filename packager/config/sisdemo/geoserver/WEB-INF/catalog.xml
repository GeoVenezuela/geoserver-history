<style filename = "Streams.sld" id = "cite_streams"  />
<style filename = "green.sld" id = "green"  />
<style filename = "usa_math.sld" id = "manualVsWorkers"  />
<style filename = "simpleRoads.sld" id = "simple_roads"  />
<style filename = "normal.sld" id = "normal"  />
<style filename = "popshade.sld" id = "population"  />
<style filename = "Ponds.sld" id = "cite_ponds"  />

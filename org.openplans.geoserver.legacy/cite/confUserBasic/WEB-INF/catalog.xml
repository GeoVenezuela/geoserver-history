<?config.xml version="1.0" encoding="UTF-8"?>
<catalog>
<datastores>
<!--
a datastore configuration element serves as a common data source connection
parameters repository for all featuretypes it holds.
-->
<datastore namespace = "topp" enabled = "true" id = "localPostgis" >
<connectionParams>
<parameter value = "postgres" name = "user"  />
<parameter value = "postgres" name = "passwd"  />
<parameter value = "localhost" name = "host"  />
<parameter value = "5432" name = "port"  />
<parameter value = "dcmms" name = "database"  />
<parameter value = "postgis" name = "dbtype"  />
</connectionParams>
</datastore>
<datastore namespace = "tiger" enabled = "true" id = "DS_giant_polygon" >
<abstract></abstract>
<connectionParams>
<parameter value = "tiger" name = "namespace"  />
<parameter value = "file:data/demo/popup_map/data/giant_polygon.shp" name = "url"  />
</connectionParams>
</datastore>


<!-- Postgis automatically puts attributes to lower case, so we have
     to adapt to that for now. -->
<xs:complexType name="OtherFeature_Type">
   <xs:complexContent>
   <xs:extension base="gml:AbstractFeatureType">
        <xs:sequence>
     <xs:element name="pointproperty" type="gml:PointPropertyType" nillable="false" minOccurs="0"/>     
    <xs:element name="mandatorystring" type="xs:string"/>
          <xs:element name="optionalstring" type="xs:string" minOccurs="0"/>
          <xs:element name="resident" type="xs:string" maxOccurs="unbounded"/>
          <xs:element name="address" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="street" minOccurs="0">
                  <xs:complexType>
                    <xs:simpleContent>
                      <xs:extension base="xs:string">
                        <xs:attribute name="number" type="xs:string" use="required"/>
                      </xs:extension>
                    </xs:simpleContent>
                  </xs:complexType>
                </xs:element>
                <xs:element name="city" type="xs:string"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="integer" type="xs:integer" minOccurs="0"/>
          <xs:element name="changeDate" type="xs:date" minOccurs="0"/>
  
        </xs:sequence>

    </xs:extension>
    
   </xs:complexContent>
 </xs:complexType>

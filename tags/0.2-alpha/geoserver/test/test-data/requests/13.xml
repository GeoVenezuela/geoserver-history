<?xml version='1.0' encoding='UTF-8'?>
<GetFeature maxFeatures="1000" xmlns:gml="http://www.opengis.net/gml">
  <Query typeName="Person">
    <PropertyName>Person/LastName</PropertyName>
    <Filter>
      <And>
        <PropertyIsGreaterThanOrEqualTo>
          <PropertyName>Person/Address/StreetNumber</PropertyName>
          <Literal>10000</Literal>
        </PropertyIsGreaterThanOrEqualTo>
        <PropertyIsLessThanOrEqualTo>
          <PropertyName>Person/Address/StreetNumber</PropertyName>
          <Literal>10999</Literal>
        </PropertyIsLessThanOrEqualTo>
        <And>
          <PropertyIsEqualTo>
            <PropertyName>Person/Address/StreetName</PropertyName>
            <Literal>Main St.</Literal>
          </PropertyIsEqualTo>
          <PropertyIsEqualTo>
            <PropertyName>Person/Address/City</PropertyName>
            <Literal>SomeTown</Literal>
          </PropertyIsEqualTo>
          <PropertyIsEqualTo>
            <PropertyName>Person/Sex</PropertyName>
            <Literal>Female</Literal>
          </PropertyIsEqualTo>
          <PropertyIsGreaterThan>
            <PropertyName>Person/Salary</PropertyName>
            <Literal>35000</Literal>
          </PropertyIsGreaterThan>
        </And>
      </And>
    </Filter>
  </Query>
</GetFeature>


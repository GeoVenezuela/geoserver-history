<?xml version="1.0" encoding="UTF-8"?>
<serverConfiguration>
  <global>
     <loggingLevel>FINER</loggingLevel>
     <maxFeatures value="20000"/>
     <verbose value="true"/>
     <numDecimals value="8"/>
     <charSet value="ISO-8859-1"/>
	 <URL>http://office.refractions.net:8081/geoserver/</URL>
  </global>
  <services>
    <service type="WFS" enabled="true">
      <name>FreeFS</name>
      <title>GeoServer TEST Web FeatureServer</title>
      <abstract>This is a test server used for cite tests</abstract>
      <keywords>
        <keyword>WFS</keyword>
        <keyword>TEST</keyword>
        <keyword>Cite</keyword>
        <keyword>GeoServer</keyword>
      </keywords>
      <onlineResource>http://office.refractions.net:8081/</onlineResource>
      <fees>NONE</fees>
      <accessConstraints>NONE</accessConstraints>
      <maintainer>Jody Garnett</maintainer>
      <URL>http://office.refractions.net:8081/geoserver/wfs</URL>
      <namespace uri="http://www.opengis.net/cite/data" prefix="cdf"/>
      <namespace uri="http://www.opengis.net/cite/geometry" prefix="cgf"/>
    </service>

    <service type="WMS" enabled="true">
      <name>FreeWMS</name>
      <title>GeoServer Cite Test Web Map Server</title>
      <abstract>This is a test server used for cite tests</abstract>
      <keywords>
        <keyword>WMS</keyword>
        <keyword>TEST</keyword>
        <keyword>Cite</keyword>
        <keyword>GeoServer</keyword>
      </keywords>
      <onlineResource>http://office.refractions.net:8081/</onlineResource>
      <fees>NONE</fees>
      <accessConstraints>NONE</accessConstraints>
      <maintainer>Jody Garnett</maintainer>
      <URL>http://office.refractions.net:8081/geoserver/wms</URL>      
    </service>
    <service type="Z39.50" enabled="false">
      <port>3210</port>
    </service>
  </services>
</serverConfiguration>

<?xml version='1.0' encoding='UTF-8'?>
<GetFeature maxFeatures="1000" xmlns:gml="http://www.opengis.net/gml">
  <Query typeName="rail">
    <PropertyName>name</PropertyName>
    <PropertyName>id</PropertyName>
  </Query>
  <Query typeName="roads">
    <PropertyName>length</PropertyName>
    <PropertyName>id</PropertyName>
  </Query>
  <Filter>
    <Within>
      <PropertyName>location</PropertyName>
      <gml:Box><gml:coordinates>50,40 60,100</gml:coordinates></gml:Box>
    </Within>
  </Filter>
</GetFeature>


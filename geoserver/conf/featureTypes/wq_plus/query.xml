<wfs:GetFeature service="WFS" version="1.0.0" outputFormat="GML2"
	xmlns:sco="http://online.socialchange.net.au"
	xmlns:wfs="http://www.opengis.net/wfs"
	xmlns:ogc="http://www.opengis.net/ogc"
	xmlns:gml="http://www.opengis.net/gml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.opengis.net/wfs
                      http://schemas.opengis.net/wfs/1.0.0/WFS-basic.xsd">
	<wfs:Query typeName="sco:wq_plus">
		<ogc:Filter>
			<ogc:PropertyIsEqualTo>
				<ogc:PropertyName>sco:station_name</ogc:PropertyName>
				<ogc:Literal>BALRANALD WEIR</ogc:Literal>
			</ogc:PropertyIsEqualTo>
		</ogc:Filter>
	</wfs:Query>
</wfs:GetFeature>
<!-- note -= station_name does not appear in output schema - its a raw column name - this needs fixing -->

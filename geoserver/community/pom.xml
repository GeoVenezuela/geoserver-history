<?xml version="1.0" encoding="ISO-8859-1"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>org.geoserver</groupId>
		<artifactId>geoserver</artifactId>
		<version>1.6.3</version>
	</parent>

	<groupId>org.geoserver</groupId>
	<artifactId>community</artifactId>
	<packaging>pom</packaging>
	<name>Community Space</name>

	<dependencies>
		<dependency>
			<groupId>org.geoserver</groupId>
			<artifactId>platform</artifactId>
			<version>1.6.3</version>
		</dependency>
	</dependencies>
	
	<profiles>
		<profile>
			<id>Java 5 Specific Plugins</id>
			<activation>
				<jdk>1.5</jdk>
			</activation>
			<modules>
				<module>sldService</module>
				<module>csv</module>
				<module>rest</module>
				<module>RESTConfig</module>
				<module>QueryUsers</module>
			</modules>
		</profile>
		<profile>
			<id>Community Schemas</id>
			<activation>
				<property>
					<name>community-schemas</name>
				</property>
			</activation>
			<modules>
				<module>community-schemas</module>
			</modules>
		</profile>
	</profiles>

	<modules>
		<module>hello</module>
		<module>kml-ows5</module>
	</modules>

</project>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <!-- 
      TYPE DEFINITIONS 
    -->
	<typedef class="org.geoserver.catalog.hibernate.FilterType" name="filter"></typedef>
    <typedef class="org.geoserver.catalog.hibernate.BoundingBoxType" name="boundingBox"></typedef>
    <typedef class="org.geoserver.catalog.hibernate.NumberRangeType" name="numberRange"></typedef>
    
    <!--
       CATALOG
    -->
    <!-- StoreInfo -->
	<class name="org.geoserver.catalog.impl.StoreInfoImpl" table="STORES">
		<cache usage="read-write" />
		<id name="id" column="STORE_ID">
            <generator class="native"/>
        </id>
        <property name="name" column="NAME" unique="true" not-null="true" />
		<property name="description" column="DESCRIPTION"/>
		<property name="enabled" column="ENABLED"/>
        <many-to-one name="workspace" column="WORKSPACE" class="org.geoserver.catalog.hibernate.HbWorkspaceInfo" lazy="false"/>
        	
        <map name="metadata" table="STORE_METADATA" cascade="all" lazy="false">
        	<key column="STORE_ID"/>
        	<map-key type="java.lang.String"></map-key>
			<element type="java.io.Serializable" length="100000"></element>
        </map>
    </class>
    
    <!-- DataStoreInfo -->
    <joined-subclass name="org.geoserver.catalog.impl.DataStoreInfoImpl" 
		extends="org.geoserver.catalog.impl.StoreInfoImpl" table="DATA_STORES">
		<key column="DATA_STORE_ID"/>
		<map name="connectionParameters" table="DATA_STORE_CONNECTION_PARAMETERS" cascade="all" lazy="false">
        	<key column="DATA_STORE_ID"/>
        	<map-key type="java.lang.String"></map-key>
			<element type="java.io.Serializable" length="100000"></element>
        </map>
		
	</joined-subclass>
    
    <!-- CoverageStoreInfo -->
    <joined-subclass name="org.geoserver.catalog.impl.CoverageStoreInfoImpl" 
		extends="org.geoserver.catalog.impl.StoreInfoImpl" table="COVERAGE_STORES">
		<key column="COVERAGE_STORE_ID"/>
		<property name="type" column="COVERAGE_STORE_TYPE"/>
		<property name="URL" column="COVERAGE_STORE_URL"/>
    </joined-subclass>
    
	<!-- StyleInfo -->
	<class name="org.geoserver.catalog.impl.StyleInfoImpl" table="STYLES">
		<cache usage="read-write" />
		<id name="id" column="STYLE_ID">
            <generator class="native"/>
        </id>
        <property name="name" column="NAME" unique="true" not-null="true"/>
        <property name="filename" column="FILENAME"/>
    </class>
    
	<!-- NamespaceInfo -->
    <class name="org.geoserver.catalog.hibernate.HbNamespaceInfo" table="NAMESPACES">
		<cache usage="read-write" />
		<id name="id" column="NS_ID">
            <generator class="native"/>
        </id>
        <property name="prefix" column="NS_PREFIX" unique="true"/>        
        <property name="URI" column="NS_URI" unique="true"/>
        <property name="default" column="IS_DEFAULT"/>        
	</class>	
	
	<!-- WorkspaceInfo -->
    <class name="org.geoserver.catalog.hibernate.HbWorkspaceInfo" table="WORKSPACES">
		<cache usage="read-write" />
		<id name="id" column="WS_ID">
            <generator class="native"/>
        </id>
        <property name="name" column="NAME" unique="true"/>  
        <property name="default" column="IS_DEFAULT"/>
        <map name="metadata" table="WORKSPACE_METADATA" cascade="all" lazy="false">
        	<key column="WS_ID"/>
        	<map-key type="java.lang.String"></map-key>
			<element type="java.io.Serializable" length="100000"></element>
        </map>      
	</class>
	
	<!-- MetadataLinkInfo -->
    <class name="org.geoserver.catalog.impl.MetadataLinkInfoImpl" table="METADATA_LINKS">
		<cache usage="read-write" />
		<id name="id" column="ID">
            <generator class="native"/>
        </id>
    	<property name="type" column="TYPE"/>
		<property name="about" column="ABOUT"/>
		<property name="metadataType" column="METADATA_TYPE"/>
		<property name="content" column="CONTENT"/>
    </class>
    
	<!-- ResourceInfo -->    
	<class name="org.geoserver.catalog.impl.ResourceInfoImpl" table="RESOURCES" abstract="true">
		<cache usage="read-write" />
		<id name="id" column="RESOURCE_ID">
            <generator class="native"/>
        </id>
        <property name="name" column="NAME" unique="true" not-null="true"/>
        <property name="nativeName" column="NATIVE_NAME" not-null="true"/>
        <!-- property name="prefixedName" column="PREFIXED_NAME"/ -->
        <many-to-one name="namespace" column="NAMESPACE" class="org.geoserver.catalog.hibernate.HbNamespaceInfo" lazy="false"/>
		<property name="title" column="TITLE"/>
		<property name="abstract" column="ABSTRACT"/>
		<property name="description" column="DESCRIPTION"/>
		<property name="enabled" column="ENABLED"/>
		<list name="alias" table="RESOURCE_ALIASES" cascade="all" lazy="false">
		   <key column="RESOURCE_ID"/>
           <list-index column="RES_ALIAS_INDEX"/>
		   <element type="java.lang.String"/>
		</list>
	    <list name="keywords" table="RESOURCE_KEYWORDS" cascade="all" lazy="false">
		   <key column="RESOURCE_ID"/>
           <list-index column="RES_KW_INDEX"/>
		   <element type="java.lang.String"/>
		</list>
		<list name="metadataLinks" table="RESOURCE_METADATA_LINK" cascade="all" lazy="false">
		   <key column="RESOURCE_ID"/>
           <list-index column="RES_METADATA_LINK_INDEX"/>
		   <many-to-many class="org.geoserver.catalog.impl.MetadataLinkInfoImpl" lazy="false"/>
		</list>
		<property name="nativeBoundingBox" type="boundingBox">
			<column name="MINX"/>
			<column name="MINY"/>
			<column name="MAXX"/>
			<column name="MAXY"/>
			<column name="CRS"/>
		</property>
		<property name="latLonBoundingBox" type="boundingBox">
			<column name="LL_MINX"/>
			<column name="LL_MINY"/>
			<column name="LL_MAXX"/>
			<column name="LL_MAXY"/>
			<column name="LL_CRS"/>
		</property>
		<property name="SRS" column="SRS"/>
		<!-- property name="CRS" column="RESOURCE_CRS" type="java.io.Serializable" length="100000"/ -->
		<property name="nativeCRS" column="NATIVE_CRS" type="java.io.Serializable" length="100000"/>
		<property name="projectionPolicy" column="PROJECTION_POLICY"/>
        
        <map name="metadata" table="RESOURCE_METADATA" cascade="all" lazy="false">
        	<key column="RESOURCE_ID"/>
        	<map-key type="java.lang.String"></map-key>
			<element type="java.io.Serializable" length="100000"></element>
        </map>
        
    	<many-to-one name="store" column="STORE" class="org.geoserver.catalog.impl.StoreInfoImpl" lazy="false"/>
	</class>
	
	<!-- FeatureTypeInfo -->
	<joined-subclass name="org.geoserver.catalog.impl.FeatureTypeInfoImpl"
		extends="org.geoserver.catalog.impl.ResourceInfoImpl" table="FEATURE_TYPES">
		<key column="RESOURCE_ID"/>
		<many-to-one name="store" column="STORE" class="org.geoserver.catalog.impl.DataStoreInfoImpl" lazy="false"/>
		<property name="filter" column="FILTER" type="filter"/>
    </joined-subclass>
    
    <!-- CoverageInfo -->
    <joined-subclass name="org.geoserver.catalog.impl.CoverageInfoImpl" 
		extends="org.geoserver.catalog.impl.ResourceInfoImpl" table="COVERAGES">
		<key column="RESOURCE_ID"/>
		<property name="nativeFormat" column="NATIVE_FORMAT"/>
		<property name="nativeSrsWKT" column="NATIVE_SRS"/>
		<property name="grid" column="GRID_GEOMETRY" type="java.io.Serializable" length="100000"/>
		<!-- property name="fields" column="FIELDS" type="org.geotools.coverage.io.impl.range.DefaultRangeType"/ -->
		
		<list name="supportedFormats" table="COVERAGE_SUPPORTED_FORMATS" cascade="all" lazy="false">
		   <key column="COVERAGE_ID"/>
           <list-index column="CV_FORMAT_INDEX"/>
		   <element type="java.lang.String"/>
		</list>
    	
    	<list name="requestSRS" table="COVERAGE_REQUEST_SRS" cascade="all" lazy="false">
			<key column="COVERAGE_ID"/>
            <list-index column="CV_REQUEST_SRS_INDEX"/>
			<element type="java.lang.String"/>
		</list>
		
		<list name="responseSRS" table="COVERAGE_RESPONSE_SRS" cascade="all" lazy="false">
			<key column="COVERAGE_ID"/>
            <list-index column="CV_RESPONSE_SRS_INDEX"/>
			<element type="java.lang.String"/>
		</list>
		
		<property name="defaultInterpolationMethod" column="DEFAULT_INTERPOLATION_METHOD"/>
		
		<list name="interpolationMethods" table="COVERAGE_INTERPOLATION_METHODS" cascade="all" lazy="false">
			<key column="COVERAGE_ID"/>
            <list-index column="CV_INTERP_METHOD_INDEX"/>
			<element type="java.lang.String"/>
		</list>
		
		<map name="parameters" table="COVERAGE_PARAMETERS" cascade="all" lazy="false">
        	<key column="COVERAGE_ID"/>
        	<map-key type="java.lang.String"></map-key>
			<element type="java.lang.String"></element>
        </map>
        
        <list name="modelRuns" table="COVERAGE_MODEL_RUNNINGS" cascade="all">
    	    <key column="COVERAGE_ID"/>
    	    <list-index column="CV_MODEL_RUN_INDEX"/>
			<one-to-many class="org.geoserver.catalog.impl.ModelRunInfoImpl"/>
    	</list>
    	
    	<list name="geophysicalParameters" table="COVERAGE_GEOPHYSIC_PARAMS" cascade="all">
		    <key column="COVERAGE_ID"/>
		    <list-index column="CV_GP_INDEX"/>
		    <many-to-many class="org.geoserver.catalog.impl.GeophysicParamInfoImpl"/>
    	</list>
    	
        <many-to-one name="store" column="STORE" class="org.geoserver.catalog.impl.CoverageStoreInfoImpl" lazy="false"/>
    </joined-subclass>
    
    <!-- LegendInfo -->
    <class name="org.geoserver.catalog.impl.LegendInfoImpl" table="LEGENDS">
	   <cache usage="read-write" />
       <id name="id" column="LEGEND_ID">
	   	 <generator class="native"/>
	   </id>
	   <property name="width" column="WIDTH"/>
	   <property name="height" column="HEIGHT"/>
	   <property name="format" column="FORMAT"/>
	   <property name="onlineResource" column="ONLINE_RESOURCE"/>
    </class>
    
    <!-- LayerInfo -->
    <class name="org.geoserver.catalog.impl.LayerInfoImpl" table="LAYERS">
	   <cache usage="read-write" />
	   <id name="id" column="LAYER_ID">
	   	 <generator class="native"/>
	   </id>
	   <property name="path" column="PATH"/>
	   <property name="name" column="NAME" unique="true" not-null="true"/>
	   <property name="type" column="TYPE" type="java.io.Serializable" length="100000"/>
	   
	   <many-to-one name="defaultStyle" column="DEFAULT_STYLE" class="org.geoserver.catalog.impl.StyleInfoImpl" lazy="false"/>
	   <set name="styles" table="LAYER_STYLES" lazy="false">
        	<key column="LAYER_ID"/>
			<many-to-many class="org.geoserver.catalog.impl.StyleInfoImpl" lazy="false"/>
       </set>   
       	   
       <many-to-one name="resource" column="RESOURCE" class="org.geoserver.catalog.impl.ResourceInfoImpl" lazy="false"/>
       
       <many-to-one name="legend" column="LEGEND" class="org.geoserver.catalog.impl.LegendInfoImpl" lazy="false"/>
    	
       <property name="enabled" column="ENABLED"/>
       
       <map name="metadata" table="LAYER_METADATA" cascade="all" lazy="false">
        	<key column="LAYER_ID"/>
        	<map-key type="java.lang.String"></map-key>
			<element type="java.io.Serializable" length="100000"></element>
        </map>
        
    </class>
    
    <!-- MapInfo -->
    <class name="org.geoserver.catalog.impl.MapInfoImpl" table="MAPS">
	   <cache usage="read-write" />
	   <id name="id" column="MAP_ID">
	   	 <generator class="native"/>
	   </id>
	   <property name="name" column="NAME" unique="true" not-null="true"/>
	   <property name="enabled" column="ENABLED"/>
       <list name="layers">
          <key column="MAP_ID"/>
          <list-index column="MAP_LAYER_INDEX"/>
		  <one-to-many class="org.geoserver.catalog.impl.LayerInfoImpl"/>
		</list>
    </class>


    <!-- ModelRunInfo -->
	<class name="org.geoserver.catalog.impl.ModelRunInfoImpl" table="MODEL_RUN">
		<cache usage="read-write" />
		<id name="id" column="MODEL_RUN_ID">
            <generator class="native"/>
        </id>
    	<property name="name" column="NAME"/>
    	<property name="description" column="DESCRIPTION"/>
    	<property name="updateSequence" column="UPDATE_SEQ"/>
    	<property name="baseTime" column="BASE_TIME"/>
    	<property name="executionTime" column="EXECUTION_TIME"/>

    <!-- ModelInfo -->
	 <class name="org.geoserver.catalog.impl.ModelInfoImpl" table="MODEL">
		<cache usage="read-write" />
		<id name="id" column="MODEL_ID">
            <generator class="native"/>
        </id>
    	<property name="abstract" column="ABSTRACT"/>    	    
    	<property name="center" column="ORIGINATING_CENTER"/>    	    
    	<property name="description" column="DESCRIPTION"/>    	    
    	<property name="discipline" column="DISCIPLINE"/>    	    

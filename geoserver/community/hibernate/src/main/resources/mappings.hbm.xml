    	<property name="TAUunit" column="TAU_UOM"/>
    	<property name="numTAU" column="TAU_NUM"/>
    	<property name="CRS" column="CRS" type="java.io.Serializable" length="100000"/>    	    
    	<property name="gridCRS" column="GRID_CRS"/>    	    
    	<property name="gridCS" column="GRID_CS"/>    	    

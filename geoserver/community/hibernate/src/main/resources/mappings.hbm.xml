			<element type="java.io.Serializable" length="100000"></element>
        </map>
        
		<map name="clientProperties" table="GEOSERVER_CLIENT_PROPS" cascade="all" lazy="false">
        	<key column="GS_ID"/>
        	<map-key type="java.lang.String"></map-key>
			<element type="java.io.Serializable" length="100000"></element>
        </map>
        
	 </class>

    <!-- ServiceInfo -->
    <class name="org.geoserver.config.impl.ServiceInfoImpl" table="SERVICES" >
		<cache usage="read-write" />
		<id name="id" column="ID"/>
        <property name="enabled" column="ENABLED"/>
		<property name="name" column="NAME" not-null="true" unique="true"/>
		<property name="title" column="TITLE"/>
		<property name="maintainer" column="MAINTAINER"/>
		<property name="abstract" column="ABSTRACT"/>	
		<property name="accessConstraints" column="ACCESS_CONSTRAINTS"/>	
		<property name="fees" column="FEES"/>	
		<list name="keywords" table="SERVICE_KEYWORDS" cascade="all" lazy="false">
		    <key column="ID"/>
		    <list-index column="SRV_KEYWORD_INDEX"/>
			<element type="java.lang.String"/>
    	</list>   
    	<list name="exceptionFormats" table="SERVICE_EXCEPTION_FORMATS" cascade="all" lazy="false">
    	    <key column="ID"/>
    	    <list-index column="SRV_EXCEPTION_FORMAT_INDEX"/>
			<element type="java.lang.String"/>
    	</list>
		<list name="versions" table="SERVICE_VERSIONS" cascade="all" lazy="false">
		    <key column="ID"/>
		    <list-index column="SRV_VERSION_INDEX"/>
			<element type="org.geotools.util.Version"/>
    	</list>       	
		<one-to-one name="metadataLink" class="org.geoserver.catalog.impl.MetadataLinkInfoImpl" lazy="false"/>
			
		<many-to-one name="geoServer" class="org.geoserver.config.impl.GeoServerInfoImpl" column="GEOSERVER" lazy="false"/>

		<property name="citeCompliant" column="CITE_COMPLIANT"/>
		<property name="onlineResource" column="ONLINE_RESOURCE"/>

		<map name="metadata" table="SERVICE_METADATA" cascade="all" lazy="false">
        	<key column="ID"/>
        	<map-key type="java.lang.String"></map-key>
			<element type="java.io.Serializable" length="100000"></element>
        </map>    
	 </class>


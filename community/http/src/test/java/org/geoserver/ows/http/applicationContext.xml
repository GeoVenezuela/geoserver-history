<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="dispatcher" class="org.geoserver.ows.http.OWSDispatcher"/>
	
	<bean id="messageKvpReader" class="org.geoserver.ows.http.MessageKvpRequestReader"/>

	<bean id="messageXmlReader" 
		class="org.geoserver.ows.http.MessageXmlParser"/>
	
	<bean id="messageResponse"
		class="org.geoserver.ows.http.MessageResponse"/>
		
	
	<bean id="helloWorld"
		class="org.geoserver.ows.http.HelloWorld"/>
		
	<bean id="helloWorldService"
		class="org.geoserver.ows.Service">
		<constructor-arg index="0" value="hello"/>
		<constructor-arg index="1" ref="helloWorld"/>
		<constructor-arg index="2" value="1.0.0"/>
	</bean>
	
</beans>
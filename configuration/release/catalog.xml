      </connectionParams>
    </datastore>
    <datastore namespace = "tiger" enabled = "true" id = "DS_poi" >
      <connectionParams >
        <parameter value = "tiger" name = "namespace" />
        <parameter value = "file:data/nyc/poi.shp" name = "url" />
      </connectionParams>
    </datastore>
    <datastore namespace = "topp" enabled = "true" id = "tasmania_cities" >

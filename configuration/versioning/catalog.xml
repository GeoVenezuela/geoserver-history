<?config.xml version="1.0" encoding="UTF-8"?>
<catalog>
<datastores>
<!--
a datastore configuration element serves as a common data source connection
parameters repository for all featuretypes it holds.
-->
<datastore namespace = "topp" enabled = "true" id = "spearfish" >
<connectionParams>
<parameter value = "topp" name = "namespace"  />
<parameter value = "true" name = "loose bbox"  />
<parameter value = "spearfish" name = "user"  />
<parameter value = "spearfish" name = "passwd"  />
<parameter value = "true" name = "wkb enabled"  />
<parameter value = "localhost" name = "host"  />
<parameter value = "5432" name = "port"  />
<parameter value = "true" name = "version enable all"  />
<parameter value = "spearfish" name = "database"  />
<parameter value = "postgis-versioned" name = "dbtype"  />
</connectionParams>
</datastore>
</datastores>
<formats>
<!--
a format configuration element serves as a common data source
parameters repository for all coverages it holds.
-->
<format namespace = "topp" enabled = "true" id = "dem" >
<type>GeoTIFF</type>
<url>file:coverages/sfdem.tif</url>
</format>
</formats>
<!--
Defines namespaces to be used by the datastores.
-->
<namespaces>
<namespace uri = "http://www.openplans.org/topp" prefix = "topp" default = "true"  />
</namespaces>
<styles>
<!--
Defines the style ids and file name to be used by the wms.
-->
<style filename = "default_line.sld" id = "line"  />
<style filename = "raster.sld" id = "raster"  />
<style filename = "default_polygon.sld" id = "polygon"  />
<style filename = "default_point.sld" id = "point"  />
</styles>
</catalog>
